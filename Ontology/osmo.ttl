osmo:pe_type_atomistic
   owl:intersectionOf (osmo:physical_equation_type [a owl:Restriction; owl:onProperty osmo:has_granularity_level; owl:hasValue osmo:ATOMISTIC]);
   owl:disjointUnionOf (
      osmo:pe_type_atomistic_density_functional     # RoMM category 2.1, OSMO PE type identifier A.1
