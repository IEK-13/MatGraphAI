<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.example.com/new_ontology.owl"
         xmlns="http://www.example.com/new_ontology.owl#">

<owl:Ontology rdf:about="http://www.example.com/new_ontology.owl"/>

<owl:AnnotationProperty rdf:about="#alternative_labels"/>

<owl:AnnotationProperty rdf:about="#name"/>

<owl:Class rdf:about="#Process">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In hydrogen technologies, a process refers to a series of steps or operations that are carried out to produce, store, or utilize hydrogen. These steps may include hydrogen production, purification, compression, storage, transportation, and utilization. Processes can be optimized to improve efficiency, reduce costs, and minimize environmental impact.</rdfs:comment>
  <alternative_labels rdf:datatype="http://www.w3.org/2001/XMLSchema#string">['Hydrogen Process', 'Hydrogen Production Process', 'Hydrogen Utilization Process', 'Hydrogen Storage Process', 'Hydrogen Compression Process']</alternative_labels>
</owl:Class>


</rdf:RDF>
